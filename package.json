{
  "name": "eedrah.github.io",
  "version": "1.0.0",
  "description": "eedrah.github.io ================",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "clean": "git rm -rf --ignore-unmatch dist",
    "prebuild": "npm run clean && cp -a content dist",
    "build": "webpack --config webpack.config.prod.js",
    "predeploy": "npm run build && git add dist && git commit -m 'deploy'",
    "deploy": "git subtree split -P dist | xargs -I % git push -f origin %:master",
    "postdeploy": "git reset --soft head^ && npm run clean",
    "dev": "webpack --config webpack.config.dev.js",
    "live": "webpack-dev-server --config webpack.config.dev.js --inline --hot",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eedrah/eedrah.github.io.git"
  },
  "author": "eedrah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eedrah/eedrah.github.io/issues"
  },
  "homepage": "https://github.com/eedrah/eedrah.github.io#readme",
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.2.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-jsx": "^1.1.1",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.1",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.9.0",
    "merge-and-concat": "^1.0.0",
    "postcss-loader": "^0.8.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
